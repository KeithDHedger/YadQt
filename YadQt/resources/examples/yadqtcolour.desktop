[Desktop Entry]
Version=1.0
Encoding=UTF-8
Name=YadQt Colour Picker
Comment=Display a QT colour chooser
Terminal=false
Type=Application
Categories=Utility;
Icon=preferences-desktop-theme
Exec=sh -c 'yadqt --type=colour --default="#ffffffff" -t "Select a colour..."|yadqt --type=input -t "Results" -b "Colour Selected" --fromstdin'
